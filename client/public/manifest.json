      "url": "/search",
      "icons": [
        {
          "src": "/icons/search-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "My Profile",
      "short_name": "Profile",
      "description": "View and edit your profile",
      "url": "/profile",
      "icons": [
        {
          "src": "/icons/profile-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Subscriptions",
      "short_name": "Subs",
      "description": "Manage your subscriptions",
